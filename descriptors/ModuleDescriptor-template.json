{
  "id": "${artifactId}-${version}",
  "name": " FOLIO translations management module",
  "provides": [{
      "id": "translations",
      "version": "1.0",
      "handlers": [{
        "methods": ["GET"],
        "pathPattern": "/translations",
        "permissionsRequired": ["translations.translations.collection.get"]
      }, {
        "methods": ["GET"],
        "pathPattern": "/translations/{id}",
        "permissionsRequired": ["translations.translations.item.get"]
      }, {
        "methods": ["POST"],
        "pathPattern": "/translations",
        "permissionsRequired": ["translations.translations.item.post"]
      }, {
        "methods": ["PUT"],
        "pathPattern": "/translations/{id}",
        "permissionsRequired": ["translations.translations.item.put"]
      }, {
        "methods": ["DELETE"],
        "pathPattern": "/translations/{id}",
        "permissionsRequired": ["translations.translations.item.delete"]
      }]
    },
    {
      "id": "languages",
      "version": "1.0",
      "handlers": [{
        "methods": ["GET"],
        "pathPattern": "/languages",
        "permissionsRequired": ["translations.languages.collection.get"]
      }, {
        "methods": ["GET"],
        "pathPattern": "/languages/{id}",
        "permissionsRequired": ["translations.languages.item.get"]
      }, {
        "methods": ["POST"],
        "pathPattern": "/languages",
        "permissionsRequired": ["translations.languages.item.post"]
      }, {
        "methods": ["PUT"],
        "pathPattern": "/languages/{id}",
        "permissionsRequired": ["translations.languages.item.put"]
      }, {
        "methods": ["DELETE"],
        "pathPattern": "/languages/{id}",
        "permissionsRequired": ["translations.languages.item.delete"]
      }]
    },
    {
      "id": "languageTranslators",
      "version": "1.0",
      "handlers": [{
        "methods": ["GET"],
        "pathPattern": "/languageTranslators",
        "permissionsRequired": ["translations.languageTranslators.collection.get"]
      }, {
        "methods": ["GET"],
        "pathPattern": "/languageTranslators/{id}",
        "permissionsRequired": ["translations.languageTranslators.item.get"]
      }, {
        "methods": ["POST"],
        "pathPattern": "/languageTranslators",
        "permissionsRequired": ["translations.languageTranslators.item.post"]
      }, {
        "methods": ["PUT"],
        "pathPattern": "/languageTranslators/{id}",
        "permissionsRequired": ["translations.languageTranslators.item.put"]
      }, {
        "methods": ["DELETE"],
        "pathPattern": "/languageTranslators/{id}",
        "permissionsRequired": ["translations.languageTranslators.item.delete"]
      }]
    },
    {
      "id": "_tenant",
      "version": "1.2",
      "interfaceType": "system",
      "handlers": [{
        "methods": ["POST"],
        "pathPattern": "/_/tenant"
      }, {
        "methods": ["DELETE"],
        "pathPattern": "/_/tenant"
      }]
    }
  ],
  "permissionSets": [{
      "permissionName": "translations.translations.collection.get",
      "displayName": "translations - get translations collection",
      "description": "get translations collection from storage"
    },
    {
      "permissionName": "translations.translations.item.get",
      "displayName": "translations - get individual translations",
      "description": "get individual translations from storage"
    },
    {
      "permissionName": "translations.translations.item.post",
      "displayName": "translations - create individual translations",
      "description": "create individual translations in storage"
    },
    {
      "permissionName": "translations.translations.item.put",
      "displayName": "translations - modify translations",
      "description": "modify translations in storage"
    },
    {
      "permissionName": "translations.translations.item.delete",
      "displayName": "translations - delete individual translations",
      "description": "delete individual translations from storage"
    },
    {
      "permissionName": "translations.languageTranslators.collection.get",
      "displayName": "translations - get locale translators collection",
      "description": "get languageTranslators collection from storage"
    },
    {
      "permissionName": "translations.languageTranslators.item.get",
      "displayName": "translations - get individual languageTranslators",
      "description": "get individual languageTranslators from storage"
    },
    {
      "permissionName": "translations.languageTranslators.item.post",
      "displayName": "translations - create individual languageTranslators",
      "description": "create individual languageTranslators in storage"
    },
    {
      "permissionName": "translations.languageTranslators.item.put",
      "displayName": "translations - modify languageTranslators",
      "description": "modify languageTranslators in storage"
    },
    {
      "permissionName": "translations.languageTranslators.item.delete",
      "displayName": "translations - delete individual languageTranslators",
      "description": "delete individual languageTranslators from storage"
    },
    {
      "permissionName": "translations.languages.collection.get",
      "displayName": "translations - get languages collection",
      "description": "get languages collection from storage"
    },
    {
      "permissionName": "translations.languages.item.get",
      "displayName": "translations - get individual languages",
      "description": "get individual languages from storage"
    },
    {
      "permissionName": "translations.languages.item.post",
      "displayName": "translations - create individual languages",
      "description": "create individual languages in storage"
    },
    {
      "permissionName": "translations.languages.item.put",
      "displayName": "translations - modify languages",
      "description": "modify languages in storage"
    },
    {
      "permissionName": "translations.languages.item.delete",
      "displayName": "translations - delete individual languages",
      "description": "delete individual languages from storage"
    },
    {
      "permissionName": "translations.all",
      "displayName": "translations module - all permissions",
      "description": "set of permissions needed to use the translations module",
      "subPermissions": [
        "translations.translations.collection.get",
        "translations.translations.item.get",
        "translations.translations.item.post",
        "translations.translations.item.put",
        "translations.translations.item.delete",
        "translations.languageTranslators.collection.get",
        "translations.languageTranslators.item.get",
        "translations.languageTranslators.item.post",
        "translations.languageTranslators.item.put",
        "translations.languageTranslators.item.delete",
        "translations.languages.collection.get",
        "translations.languages.item.get",
        "translations.languages.item.post",
        "translations.languages.item.put",
        "translations.languages.item.delete"
      ]
    }
  ],
  "metadata": {
    "containerMemory": "512",
    "databaseConnection": "true"
  },
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerArgs": {
      "HostConfig": {
        "PortBindings": {
          "8081/tcp": [{
            "HostPort": "%p"
          }]
        }
      }
    },
    "dockerPull": false
  }
}