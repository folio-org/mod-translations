{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A collection of languages and their assigned translators",
  "type": "object",
  "properties": {
    "languageTranslators": {
      "description": "List of languages translators",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "languageTranslator.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    }
  },
  "required": [
    "languageTranslators",
    "totalRecords"
  ]
}
